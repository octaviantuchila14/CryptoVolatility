<p id="notice"><%= notice %></p>

<h1>Listing Neural Networks</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @neural_networks.each do |neural_network| %>
      <tr>
        <td><%= link_to 'Show', neural_network %></td>
        <td><%= link_to 'Edit', edit_neural_network_path(neural_network) %></td>
        <td><%= link_to 'Destroy', neural_network, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Neural network', new_neural_network_path %>
